{
  "name": "filmee_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create filmee_backend && dfx generate filmee_backend && dfx deploy",
    "start": "vite --port 3000",
    "prebuild": "dfx generate",
    "tailwind": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css --watch",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\""
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@dfinity/candid": "^2.1.3",
    "@dfinity/ledger-icp": "^2.6.7",
    "@dfinity/principal": "^2.1.3",
    "@tailwindcss/cli": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.3.1",
    "postcss": "^8.5.1",
    "sass": "^1.63.6",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  }
